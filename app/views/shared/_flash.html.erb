<% if flash && flash.keys.any? %>
<section id="content">
  <div class="content-wrap">
    <% flash.each do |key, value| %>
      <div class="<%= flash_class(key) %>">
        <div class="container">
          <div class="row">
            <div class="col-md-12 col-lg-12">
              <div class="sb-msg"><%= value %></div>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</section>
<% end %>
